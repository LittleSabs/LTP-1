cliente = {
    'id':0,
    'nome': 'Pedro',
    'mensalidade': 249.9,
    'ativo': True,
    'altura': 1.78,
    'peso':86
}
print(cliente['nome'])
print(cliente.get('nome'))

print(cliente.items())
for chave, valor in cliente.items():
    print(f'{chave} : {valor}')

print(cliente.keys())
for chave in cliente.keys():
    print(chave)

print(cliente.values())
for valor in cliente.values():
    print (valor)

cliente.update({'ativo': False})
cliente.upate({'emdere√ßo': 'Quadra 2'})
print(cliente)
