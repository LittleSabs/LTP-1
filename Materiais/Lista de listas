cadastro = []
cliente1 = [0,'Pedro',259.99, True, 1.78, 83]
cliente2 = [1,'Ana',279.99, False, 1.65, 61]
 
cadastro.append(cliente1)
cadastro.append(cliente2)
print(cadastro)
 
for cliente in cadastro:
    print(cliente)
 
print(cadastro[1])
print(cadastro[1][3])

